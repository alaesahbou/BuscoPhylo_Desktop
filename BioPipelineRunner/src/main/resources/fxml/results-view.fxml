<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" spacing="10" alignment="CENTER">
    <Label text="Results" styleClass="header"/>
    <TextArea fx:id="resultsTextArea" wrapText="true" editable="false" prefWidth="600" prefHeight="400"/>
    <Button text="Visualize Tree" onAction="#handleVisualizeTree"/>
    <Button text="Export Results" onAction="#handleExportResults"/>
</VBox>